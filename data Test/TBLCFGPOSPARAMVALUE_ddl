CREATE TABLE IRIS.TBLCFGPOSPARAMVALUE
(
  PARAM_ID        VARCHAR2(20 BYTE)             NOT NULL,
  INSTITUTION_ID  VARCHAR2(20 BYTE)             NOT NULL,
  VERSION         VARCHAR2(20 BYTE)             NOT NULL,
  VALUE           VARCHAR2(500 BYTE),
  SERIAL          VARCHAR2(20 BYTE)             NOT NULL,
  SUPPLEMENTAL LOG DATA (PRIMARY KEY) COLUMNS
)
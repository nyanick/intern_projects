
CREATE TABLE IRIS.TBLPOSPARAMVERSION
(
  POS_ID           VARCHAR2(20 BYTE)            NOT NULL,
  TABLE_ID         VARCHAR2(20 BYTE)            NOT NULL,
  VERSION          VARCHAR2(20 BYTE)            NOT NULL,
  PROFILE_ID       VARCHAR2(20 BYTE),
  UPDATE_REQUIRED  VARCHAR2(1 BYTE),
  SUPPLEMENTAL LOG DATA (PRIMARY KEY) COLUMNS
)
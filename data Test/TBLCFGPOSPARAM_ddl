CREATE TABLE IRIS.TBLCFGPOSPARAM
(
  TABLE_ID         VARCHAR2(20 BYTE),
  TABLE_NAME       VARCHAR2(250 BYTE),
  TAG              VARCHAR2(50 BYTE),
  TAG_ID           VARCHAR2(20 BYTE),
  PARAM_NAME       VARCHAR2(100 BYTE),
  TAG_SEQ          VARCHAR2(20 BYTE),
  PARAM_SEQ        VARCHAR2(20 BYTE),
  MAX_LENGTH       VARCHAR2(20 BYTE),
  POSSIBLE_VALUES  VARCHAR2(250 BYTE),
  DISPLAYCONTROL   VARCHAR2(20 BYTE),
  PARAM_ID         VARCHAR2(20 BYTE)            NOT NULL,
  DATA_TYPE        VARCHAR2(20 BYTE),
  DATA_ENCODING    VARCHAR2(20 BYTE),
  LENGTH_TYPE      VARCHAR2(20 BYTE),
  REG_EX           VARCHAR2(1024 BYTE),
  SUPPLEMENTAL LOG DATA (PRIMARY KEY) COLUMNS
)